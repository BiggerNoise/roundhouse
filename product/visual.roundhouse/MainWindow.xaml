<Window x:Class="visual.roundhouse.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:UserControls="clr-namespace:visual.roundhouse.UserControls" Title="Visual RoundhousE" Height="300" Width="300" Icon="/visual.roundhouse;component/roundhouse.ico">
    <DockPanel>
        <Menu DockPanel.Dock="Top" VerticalAlignment="Top" >
            <MenuItem Header="_File">
                <MenuItem Header="_Open" IsCheckable="true" Click="FileOpenMenu_Click"/>
            </MenuItem>
        </Menu>
              
        <Grid DockPanel.Dock="Bottom">
            <UserControls:OpenKicksTabControl x:Name="openKicksUi" />
        </Grid>
    </DockPanel>
</Window>
